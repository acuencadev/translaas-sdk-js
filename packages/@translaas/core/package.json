{
  "name": "@translaas/core",
  "version": "0.1.0",
  "description": "Main package for the Translaas SDK - re-exports all packages",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -p tsconfig.json --watch",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@translaas/models": "workspace:*",
    "@translaas/client": "workspace:*",
    "@translaas/caching": "workspace:*",
    "@translaas/caching-file": "workspace:*",
    "@translaas/extensions": "workspace:*"
  },
  "keywords": [
    "translaas",
    "translation",
    "i18n",
    "localization",
    "sdk",
    "typescript",
    "javascript"
  ],
  "author": "Translaas SDK Contributors",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
