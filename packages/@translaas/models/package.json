{
  "name": "@translaas/models",
  "version": "0.3.0",
  "description": "Data transfer objects (DTOs) and models for the Translaas Translation Delivery API",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs && npm run build:copy-cjs",
    "build:esm": "tsc --build tsconfig.json",
    "build:cjs": "tsc --build tsconfig.cjs.json",
    "build:copy-cjs": "node ../../../scripts/copy-cjs.js",
    "build:watch": "tsc --build tsconfig.json --watch",
    "clean": "rm -rf dist dist-cjs",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage --passWithNoTests"
  },
  "keywords": [
    "translaas",
    "translation",
    "i18n",
    "models",
    "dto"
  ],
  "author": "Translaas SDK Contributors",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
